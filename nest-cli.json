{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dias/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/dias/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "dias": {
      "type": "application",
      "root": "apps/dias",
      "entryFile": "main",
      "sourceRoot": "apps/dias/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dias/tsconfig.app.json"
      }
    },
    "asset-server": {
      "type": "application",
      "root": "apps/asset-server",
      "entryFile": "main",
      "sourceRoot": "apps/asset-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/asset-server/tsconfig.app.json"
      }
    },
    "admin-server": {
      "type": "application",
      "root": "apps/admin-server",
      "entryFile": "main",
      "sourceRoot": "apps/admin-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-server/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/dias"
}