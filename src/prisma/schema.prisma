generator client {
  provider = "prisma-client-js"
  output = ""
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_DB_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  userId    String  @unique
  name      String
  email     String  @unique
  password  String
  isStudent Boolean @default(true)
  number    Int?    @unique
}
